    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-prettier": "0.4.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-typescript2": "0.19.3",
    "ts-node": "7.0.1",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
